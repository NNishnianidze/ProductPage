{
    "require": {
        "ext-pdo": "*",
        "doctrine/dbal": "^3.4",
        "doctrine/migrations": "^3.5",
        "doctrine/orm": "^2.13",
        "php-di/php-di": "^6.4",
        "symfony/cache": "^6.1",
        "symfony/console": "^6.1",
        "twig/intl-extra": "^3.4",
        "twig/twig": "^3.4",
        "vlucas/phpdotenv": "^5.3",
        "vlucas/valitron": "^1.4"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5"
    }
}
